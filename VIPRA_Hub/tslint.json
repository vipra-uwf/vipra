{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "trailing-comma": [ false ],
        "semicolon": [ true, "always" ],
        "no-empty":true,
        "no-console":false,
        "no-async-without-await":true,
        "no-floating-promises":true,
        "arrow-parens":true
    },
    "rulesDirectory": []
}